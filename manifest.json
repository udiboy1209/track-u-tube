{
	"manifest_version":2,
	"version":"1.0",
	
	"name":"Youtube API Test",
	"description":"Trying out APIs",

	"browser_action":
	{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},
	"background":{
		"scripts":["jquery.js"]
	},
	"permissions":[
		"tabs", "https://www.googleapis.com/*","https://apis.google.com/*","identity","https://accounts.google.com/o/oauth2/"
	],
	"oauth2": {
		"client_id": "856154260669-drv0jdct8udp514ojulg50mofsvgqgf5.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/userinfo.email",
			"https://www.googleapis.com/auth/youtube"
		]
	},
	"content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}